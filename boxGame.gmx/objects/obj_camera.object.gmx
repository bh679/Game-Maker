            <string>xfrom = 0
yfrom = 0
zfrom = 0;

// Start up camera
d3d_start()
d3d_set_hidden(1)
d3d_set_culling(0)
d3d_set_lighting(1)
d3d_set_shading(1)
d3d_set_fog(0,c_dkgray,0,300)
d3d_light_enable(0,1)

camangle=45
camanglez=25
camzoom=100
camdownanglez = camanglez
camdownangle = camangle

// Variables for view dragging
drag=0
prevx=mouse_x
prevy=mouse_y

side = 0
text = ''

exitmenu = menu_new()
menu_item_add(exitmenu,"X");
menu_font(exitmenu,font_UI,c_white,0.9)
menu_selectbox(exitmenu,c_white,0.5)
menu_padding(exitmenu,menu_get_hpadding(exitmenu),24*2)
menu_position(exitmenu, 740,0)
</string>
