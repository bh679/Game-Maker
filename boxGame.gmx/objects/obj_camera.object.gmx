        <arguments>
          <argument>
            <kind>1</kind>
            <string>draw_clear(c_black)
// Set the position of the camera.
xfrom=cos(degtorad(camanglez))*lengthdir_x(camzoom,camangle+180)
yfrom=cos(degtorad(camanglez))*lengthdir_y(camzoom,camangle+180)
zfrom=sin(degtorad(camanglez))*camzoom

// Set light and projection, with a FOV of 60.
draw_set_color(c_white)
draw_set_alpha(1)
d3d_set_projection_ext(xfrom,yfrom,zfrom,0,0,0,0,0,1,60,1920/1280,1,32000)
d3d_light_define_point(0,xfrom,yfrom,zfrom,2000,c_white)

//draw grid

clicked = noone
side = 0
with(obj_box)
{
    // Prepare the block checking script, notice how we use the same arguments
    // as in the projection function, with the exception of the last three.
    d3d_check_mouse_block_prepare(obj_camera.xfrom,obj_camera.yfrom,obj_camera.zfrom,0,0,0,0,0,1,60,0)
    // Draw a block, check the cursor and store the result into a variable.
    draw_set_color(image_blend)
    d3d_draw_block(x-8,y-8,z-8,x+8,y+8,z+8,-1,1,1)
    side = d3d_check_mouse_block(x-8,y-8,z-8,x+8,y+8,z+8)
    if side &gt; 0
    {
        obj_camera.clicked = d3d_closest_to_camera(obj_camera.clicked,id)
        if (id = obj_camera.clicked)
        {
            obj_camera.side = side
        }
    }
}

draw_set_alpha(1)
//targets
for(i = 0; i &lt; targets; i += 1)
{
    draw_set_color(c_white)
    if target[i] = false
        d3d_draw_block(targetx[i]*blocksize-8,targety[i]*blocksize-8,
                        targetz[i]*blocksize-8,targetx[i]*blocksize+8,
                        targety[i]*blocksize+8,targetz[i]*blocksize+8,
                        sprite_get_texture(spr_outline,0),1,1)
}
draw_set_alpha(1)

//highlights
with(clicked)
{
    // Highlight the selected side.
    d3d_set_lighting(0)
    draw_set_color(c_red)
    text=""
    if (side=1) {text="East (X)" /*d3d_draw_wall(x+8.01,y-8.01,z-8.01,x+8.01,y+8.01,z+8.01,-1,1,1)*/}
    else if (side=2) {text="West (-X)" /*d3d_draw_wall(x-8.01,y-8.01,z-8.01,x-8.01,y+8.01,z+8.01,-1,1,1)*/}
    else if (side=3) {text="South (Y)" /*d3d_draw_wall(x-8.01,y+8.01,z-8.01,x+8.01,y+8.01,z+8.01,-1,1,1)*/}
    else if (side=4) {text="North (-Y)" /*d3d_draw_wall(x-8.01,y-8.01,z-8.01,x+8.01,y-8,z+8.01,-1,1,1)*/}
    else if (side=5) {text="Above (Z)" /*d3d_draw_floor(x-8.01,y-8.01,z+8.01,x+8.01,y+8,z+8.01,-1,1,1)*/}
    else if (side=6) {text="Below (-Z)" /*d3d_draw_floor(x-8.01,y-8.01,z-8.01,x+8.01,y+8.01,z-8.01,-1,1,1)*/}
}

//nearest test
/*with(d3d_instance_nearest(targetx[1]*blocksize,targety[1]*blocksize,targetz[1]*blocksize,obj_box))
{
    draw_set_color(c_blue)
    d3d_draw_block(x-9,y-9,z-9,x+9,y+9,z+9,sprite_get_texture(spr_alpha,0),1,1)
}*/


// Draw some text.
d3d_set_lighting(0)
draw_set_font(font_UI)
draw_set_halign(fa_left)
draw_set_valign(fa_top)
d3d_set_projection_ortho(0,0,view_wview,view_hview,0)
d3d_set_hidden(1)
draw_set_color(c_white)
//if (text!="") draw_text(2,2,"Touching side: "+text)
//if obj_camera.side &gt; 0 draw_text(2,2,"Mouse on")
draw_text(10,10,"blocks: " + string(instance_number(obj_box)) + " / " + string(blocks))
draw_text(10,50,"Levels Completed: "+string(score) + "mouse: " + string(mouse_x) + " " + string(mouse_y))
if os_type != os_ios and os_type != os_browser draw_menu(exitmenu)
//slider
draw_rectangle(view_wview/3, view_hview - 50, view_wview - view_wview/3, view_hview,true)
zoom_x = ((camzoom - 10)/990)*(view_wview/3) + view_wview/3// = mouse_x - view_wview/3
draw_rectangle(max(zoom_x-10,view_wview/3-25), view_hview - 50, min(view_wview - view_wview/3+10,zoom_x+25), view_hview,false)

d3d_set_hidden(1)
d3d_set_lighting(1)

// Show FPS.
//room_caption="FPS: "+string(fps)*/
</string>
          </argument>
