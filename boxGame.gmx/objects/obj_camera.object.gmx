    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Zoom with mouse wheel.
if (mouse_wheel_up() &amp;&amp; camzoom&gt;10) camzoom/=1.2
if (mouse_wheel_down() &amp;&amp; camzoom&lt;1000) camzoom*=1.2

//zoom with bar
if MouseInBox(view_wview/3, view_hview - 50, view_wview - view_wview/3, view_hview) and mouse_check_button(mb_left)
{
    camzoom = ((mouse_x - view_wview/3)/(view_wview/3))*990 + 10
    drag = 0
}

if (mouse_check_button_released(mb_left) &amp;&amp; side &gt; 0 &amp;&amp; point_distance(camdownanglez,camdownangle,camanglez,camangle) &lt; 1)
{
    newx = 0
    newy = 0
    newz = 0
    if (side=1) {newx += blocksize}
    else if (side=2) {newx -= blocksize}
    else if (side=3) {newy += blocksize}
    else if (side=4) {newy -= blocksize}
    else if (side=5) {newz += blocksize}
    else if (side=6) {newz -= blocksize}
    newblock = instance_create(clicked.x + newx,clicked.y + newy,obj_box)
    newblock.z = clicked.z + newz
    
    if instance_number(obj_box) &gt; blocks
        newblock.image_blend = c_red
}


// Drag view with left mouse button.
if (drag=1) {
    if (mouse_check_button(mb_left)) {
        camangle-=(mouse_x-prevx)/view_wview*400
        camanglez+=(mouse_y-prevy)/view_hview*300
        camanglez=median(89.999,-89.999,camanglez)
    } else {
        drag=0
    }
} else if (mouse_check_button_pressed(mb_left)) 
{
    camdownanglez = camanglez
    camdownangle = camangle
    drag = 1
}

finished = true
for(i = 0; i &lt; targets; i += 1)
{
    finished *= target[i]
}
if finished = true
{
    if instance_number(obj_box) &lt;= blocks
        score += 1
    room_restart()
}

prevx=mouse_x
prevy=mouse_y

if (os_type != os_ios and os_type != os_browser) and menu_item_clicked(exitmenu,"X") || keyboard_check(vk_escape)
{
    game_end()
}


</string>
          </argument>
