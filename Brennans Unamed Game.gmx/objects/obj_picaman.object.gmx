        <arguments>
          <argument>
            <kind>1</kind>
            <string>obj_earth.angleSpeed = 0
image_blend = c_white
 
//eat
if keyboard_check(vk_space) and eatMan = false and instance_number(obj_dragon) = 0
{
    //eat closest man
    closestMan = instance_nearest(x,y,obj_enemy)
    
    //next food
    nextFood = instance_nearest(x,y,obj_notfood)
    i = 0//keep eadiable
    while((nextFood.angle - obj_earth.angle) &lt; 2 and nextFood.level &gt;= food/100+1 and i &lt; instance_number(obj_notfood)-1)
    {
        i+=1
        nextFood = instance_nth_nearest(x,y,obj_notfood,i)
    }
    
    if distance_to_object(closestMan) &lt; max(2*obj_earth.scale,4) and eatStill = false
    {
        eatStill = true
        eatMan = true
        image_index = 0
        
        //choose kill type
        var types;
        types = 3;
        switch(floor(random(types)))
        {
            case 0:
                sprPicaman = spr_toss2
                image_speed = 0.3
                break;
            case 1:
                sprPicaman = spr_toss
                image_speed = 0.3
                break;
            case 2:
                sprPicaman = spr_disembowel
                image_speed = 0.4
                break;
            default:
                sprPicaman = spr_toss2
                image_speed = 0.2
                break;
            
        }
        
        //kill man
        with(closestMan)
        {
            instance_destroy()
        }
    }
    //eat scene
    if distance_to_object(nextFood) &lt;  max(10*obj_earth.scale,1) and eatMan = false
    {
        image_blend = nextFood.colour
        eatStill = true
        sprPicaman = sprEat
        obj_earth.angleSpeed = 0
        image_speed = 0.5
        nextFood._health -= 1
        food += 1
    }
}

if eatStill = false
{
   sprPicaman = sprStanding
    image_speed = 0.3    

    //right
    if keyboard_check(vk_right)
    {
        //spin earth
        obj_earth.angleSpeed = 0.5
        
        //move man
        sprPicaman = sprRunning
        image_speed = obj_earth.angleSpeed //speed of man
        image_xscale = scale//direction facing
        
    }
    else if keyboard_check(vk_left)
    {
        //spin earth
        obj_earth.angleSpeed = -0.5
        
        //move man
        sprPicaman = sprRunning
        image_speed = obj_earth.angleSpeed //speed of man
        image_xscale = -scale//direction facing
    }
}

obj_earth.angle += obj_earth.angleSpeed
sprite_index = sprPicaman
</string>
