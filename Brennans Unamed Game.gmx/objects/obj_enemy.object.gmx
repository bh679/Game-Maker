        image_speed = 0.3
        standAttacking = true
    }
}

//get target speed
if image_angle mod 360 &lt; 180
{
    if obj_earth.angleSpeed &lt; 0
        targetSpeed = -_withSpeed
    else
        targetSpeed = -_againstSpeed
    image_xscale = scale
}
else if image_angle mod 360 &gt; 180
{

    if obj_earth.angleSpeed &lt; 0
        targetSpeed = _againstSpeed
    else
        targetSpeed = _withSpeed
    image_xscale = -scale
}

//accellerate
if targetSpeed &gt; 0
{
    if _speed &lt; targetSpeed
        _speed += 0.05
    else if _speed &gt; targetSpeed
        _speed -= 0.05

}
else if targetSpeed &lt; 0
{

    if _speed &lt; targetSpeed
        _speed += 0.05
    else
        _speed -= 0.05
}
else
    _speed = 0

//move
//if standAttacking = false
    angle += _speed

if (obj_earth.angle-angle &gt; 180)
    instance_destroy()
    
    
count += 1
</string>
